<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
    <mime-type type="application/atari-st-rom-cartridge">
        <sub-class-of type="application/octet-stream"/>
        <comment>Atari ST ROM cartridge file</comment>
        <magic priority="50">
            <match type="big32" offset="0" value="0x00000000">
                <match type="big32" offset="4" value="0xabcdef42"/>
            </match>
        </magic>
        <glob pattern="*.STC"/>
        <generic-icon name="atari-st-rom-cartridge"/>
    </mime-type>
</mime-info>
