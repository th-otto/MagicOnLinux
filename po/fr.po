#: ../src/gui/preferences.xml:3
msgid  "Host Paths"
msgstr "Chemins d'hôte"

#: ../src/gui/preferences.xml:3
msgid  "Configure kernel, rootfs, and temporary paths on the host system"
msgstr "Configure les chemins du noyau, du rootfs et temporaires sur le système hôte"

#: ../src/gui/preferences.xml:4
msgid  "Kernel File (ROM)"
msgstr "Fichier Kernel (ROM)"

#: ../src/gui/preferences.xml:4
msgid  ""
"Path to the Atari Kernel/ROM file.\n"
"If empty, MAGICLIN.OS in the root filesystem will be used."
msgstr ""
"Chemin d'accès au fichier noyau/ROM Atari.\n"
"Si ce champ est vide, le fichier MAGICLIN.OS situé à la racine du système de fichiers sera utilisé."

#: ../src/gui/preferences.xml:5
msgid  "Path for boot filesystem (Folder)"
msgstr "Système de fichiers racine (Dossier)"

#: ../src/gui/preferences.xml:5
msgid  "Path to the host directory containing the root filesystem of the emulation"
msgstr "Chemin d'accès au répertoire hôte contenant le système de fichiers racine de l'émulation"

#: ../src/gui/preferences.xml:6
msgid  "Mount $HOME directory as drive H:"
msgstr "Monter le répertoire $HOME comme lecteur H:"

#: ../src/gui/preferences.xml:6
msgid  "If enabled, mounts the host home directory (~/) as U:/home inside the emulation"
msgstr "Si activé, monte le répertoire d'accueil hôte (~/) comme U:/home à l'intérieur de l'émulation"

#: ../src/gui/preferences.xml:7
msgid  "Set HOME directory read-only"
msgstr "Dossier d'accueil hôte en lecture seule"

#: ../src/gui/preferences.xml:7
msgid  "Mounts the host home directory as read-only. Requires 'Mount $HOME...' to be enabled"
msgstr "Monte le répertoire d'accueil hôte en lecture seule. Nécessite que 'Mount $HOME..."

#: ../src/gui/preferences.xml:8
msgid  "Mount host root directory (/)"
msgstr "Monter le chemin racine de l'hôte (/)"

#: ../src/gui/preferences.xml:8
msgid  "If enabled, mounts the host root directory (/) for full system access"
msgstr "Si activé, monte le répertoire racine hôte (/) pour un accès complet au système"

#: ../src/gui/preferences.xml:9
msgid  "Set root directory read-only"
msgstr "Chemin racine hôte en lecture seule"

#: ../src/gui/preferences.xml:9
msgid  "Mounts the host root directory as read-only. Requires 'Mount Host Root Path' to be enabled"
msgstr "Monte le répertoire racine hôte en lecture seule. Nécessite que 'Mount Host Root Path' soit activé"

#: ../src/gui/preferences.xml:10
msgid  "Path for temp files"
msgstr "Chemin d'hôte temporäre"

#: ../src/gui/preferences.xml:10
msgid  "Host directory used by the emulation for temporary files"
msgstr "Répertoire hôte utilisé par l'émulation pour les fichiers temporären"

#: ../src/gui/preferences.xml:12
msgid  "Host Devices"
msgstr "Périphériques d'hôte"

#: ../src/gui/preferences.xml:12
msgid  "Set the host device paths for serial communication and printing"
msgstr "Définit les chemins des périphériques hôtes pour la communication série et l'impression"

#: ../src/gui/preferences.xml:13
msgid  "Print command (program/file)"
msgstr "Commande d'impression (Programmes/Fichier)"

#: ../src/gui/preferences.xml:13
msgid  "Command or device path used to handle printing output from the emulation"
msgstr "Commande ou chemin de périphérique utilisé pour gérer la sortie d'impression de l'émulation"

#: ../src/gui/preferences.xml:14
msgid  "Serial device (e.g., /dev/ttyS0)"
msgstr "Fichier périphérique série (p. ex. /dev/ttyS0)"

#: ../src/gui/preferences.xml:14
msgid  "Path to the host serial device (e.g., /dev/ttyS0) for serial communication"
msgstr "Chemin d'accès au périphérique série hôte (p. ex. /dev/ttyS0) pour la serielle Kommunikation"

#: ../src/gui/preferences.xml:16
msgid  "Atari Screen"
msgstr "Écran Atari"

#: ../src/gui/preferences.xml:16
msgid  "Adjust the Atari screen resolution, refresh rate, scaling, and mouse hiding"
msgstr ""
"Ajuste la résolution de l'écran Atari, le taux de rafraîchissement, la mise à l'échelle et le masquage de la souris"

#: ../src/gui/preferences.xml:17
msgid  "Resolution (Width in Pixels)"
msgstr "Résolution (Largeur en pixels)"

#: ../src/gui/preferences.xml:17
#: ../src/gui/preferences.xml:18
msgid  "The display resolution in pixels for the Atari screen window"
msgstr "La résolution d'affichage en pixels pour la fenêtre d'écran Atari"

#: ../src/gui/preferences.xml:18
msgid  "Resolution (Height in Pixels)"
msgstr "Résolution (Hauteur en pixels)"

#: ../src/gui/preferences.xml:19
msgid  "X Stretch Factor (Scaling)"
msgstr "Facteur d'étirement X (Mise à l'échelle)"

#: ../src/gui/preferences.xml:19
msgid  "Horizontal scaling factor (1 = normal, 2 = double width, etc.)"
msgstr "Facteur d'étirement X (Mise à l'échelle)"

#: ../src/gui/preferences.xml:20
msgid  "Y Stretch Factor (Scaling)"
msgstr "Facteur d'étirement Y (Mise à l'échelle)"

#: ../src/gui/preferences.xml:20
msgid  "Vertical scaling factor (1 = normal, 2 = double height, etc"
msgstr "Facteur d'étirement Y (Mise à l'échelle)"

#: ../src/gui/preferences.xml:21
msgid  "Refresh Rate (Hz)"
msgstr "Taux de rafraîchissement (Hz)"

#: ../src/gui/preferences.xml:21
msgid  "Target screen refresh rate in Hertz (Hz)"
msgstr "Taux de rafraîchissement cible en Hertz (Hz)"

#: ../src/gui/preferences.xml:22
msgid  "Screen color mode"
msgstr "Profondeur/Mode couleur"

#: ../src/gui/preferences.xml:22
msgid  "Select the color depth or graphics mode used for the screen"
msgstr "Sélectionnez la profondeur de couleur ou le mode graphique utilisé pour l'écran"

#: ../src/gui/preferences.xml:23
msgid  "24-bit true color"
msgstr "24 bits TrueColor"

#: ../src/gui/preferences.xml:24
msgid  "16-bit high color"
msgstr "16 bits HighColor"

#: ../src/gui/preferences.xml:25
msgid  "256 colors, packed pixels"
msgstr "256 Couleurs, pixels compactés"

#: ../src/gui/preferences.xml:26
msgid  "16 colors, packed pixels"
msgstr "16 Couleurs, pixels compactés"

#: ../src/gui/preferences.xml:27
msgid  "16 colors, interleaved planes"
msgstr "16 couleurs, plans entrelacés"

#: ../src/gui/preferences.xml:28
msgid  "4 colors, interleaved planes"
msgstr "4 couleurs, plans entrelacés"

#: ../src/gui/preferences.xml:29
msgid  "monochrome"
msgstr "Monochrome"

#: ../src/gui/preferences.xml:31
msgid  "Hide host mouse"
msgstr "Masquer le pointeur de la souris hôte"

#: ../src/gui/preferences.xml:31
msgid  "Hides the host operating system's mouse pointer when it is over the emulation window"
msgstr ""
"Masquer le pointeur de la souris du système d'exploitation hôte lorsqu'il est au-dessus de la fenêtre d'émulation"

#: ../src/gui/preferences.xml:32
msgid  "Use relative mousemode"
msgstr "Utiliser le mode souris relatif"

#: ../src/gui/preferences.xml:32
msgid  "Tells SDL to send relative mouse movements"
msgstr "Indique à SDL d'envoyer les mouvements relatifs de la souris."

#: ../src/gui/preferences.xml:34
msgid  "Window Placement"
msgstr "Placement de la fenêtre"

#: ../src/gui/preferences.xml:34
msgid  "Define the window's position (X/Y) and which display it should appear on"
msgstr "Définit la position (X/Y) et l'affichage sur lequel la fenêtre doit apparaître"

#: ../src/gui/preferences.xml:35
msgid  "Monitor"
msgstr "Moniteur"

#: ../src/gui/preferences.xml:35
msgid  "The display number (e.g., 0, 1, 2) on which the window should appear"
msgstr "Le numéro d'affichage (p. ex. 0, 1, 2) sur lequel la fenêtre doit apparaître"

#: ../src/gui/preferences.xml:36
msgid  "Window position X (left)"
msgstr "Position de la fenêtre X (gauche)"

#: ../src/gui/preferences.xml:36
#: ../src/gui/preferences.xml:37
msgid  "Window position coordinates relative to the top-left corner of the screen"
msgstr "Coordonnées de position de la fenêtre par rapport au coin supérieur gauche de l'écran"

#: ../src/gui/preferences.xml:37
msgid  "Window position Y (top)"
msgstr "Position de la fenêtre Y (haut)"

#: ../src/gui/preferences.xml:39
msgid  "Atari Emulation"
msgstr "Émulation Atari"

#: ../src/gui/preferences.xml:39
msgid  "General emulation settings, including RAM size, language ROM, and menu behavior"
msgstr "Paramètres d'émulation généraux, y compris la taille de la RAM, la ROM de langue et le comportement du menu"

#: ../src/gui/preferences.xml:40
msgid  "Memory size (RAM in bytes)"
msgstr "Taille de la mémoire (RAM en octets)"

#: ../src/gui/preferences.xml:40
msgid  "Amount of RAM allocated to the emulated system (e.g., 8MB, 32MB)"
msgstr "Quantité de RAM allouée au système émulé (p. ex. 8MB, 32MB)"

#: ../src/gui/preferences.xml:41
msgid  "Atari language"
msgstr "Langue Atari"

#: ../src/gui/preferences.xml:41
msgid  "Selects the language ROM to be used by the emulation (affects keyboard layout and startup language)"
msgstr ""
"Sélectionne la ROM de langue à utiliser par l'émulation (affecte la disposition du clavier et la langue de démarrage)"

