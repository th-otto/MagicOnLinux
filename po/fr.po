msgstr "_Annuler"

#: ../src/gui/gxmessage.c:1178
#, c-format
msgid  ""
"\n"
"%s - a GTK-based xmessage clone\n"
msgstr ""
"\n"
"%s - un clone xmessage basé sur GTK\n"

#: ../src/gui/gxmessage.c:1180
#, c-format
msgid  "Usage: %s [OPTIONS] message ...\n"
msgstr "Utilisation: %s [OPTIONS] message ...\n"

#: ../src/gui/gxmessage.c:1181
#, c-format
msgid  "       %s [OPTIONS] -file FILENAME\n"
msgstr "             %s [OPTIONS] -file NOM_FICHIER\n"

#: ../src/gui/gxmessage.c:1183
#, c-format
msgid  "xmessage options:\n"
msgstr "Options xmessage:\n"

#: ../src/gui/gxmessage.c:1184
#, c-format
msgid  "  -file FILENAME         Get message text from file, '-' for stdin\n"
msgstr ""
"  -file NOM_DU_FICHIER   Récupérer le texte du message depuis un fichier, '-' pour l'entrée "
"standard\n"

#: ../src/gui/gxmessage.c:1185
#, c-format
msgid  "  -buttons BUTTON_LIST   List of \"LABEL:EXIT_CODE\", comma separated\n"
msgstr "  -buttons BUTTON_LIST   Liste de \"LABEL:EXIT_CODE\", séparés par des virgules\n"

#: ../src/gui/gxmessage.c:1186
#, c-format
msgid  "  -default LABEL         Give keyboard focus to the specified button\n"
msgstr "  -default LABEL         Donner le focus clavier au bouton spécifié\n"

#: ../src/gui/gxmessage.c:1187
#, c-format
msgid  "  -print                 Send the selected button's LABEL to stdout\n"
msgstr ""
"  -print                 Afficher l'étiquette du bouton sélectionné sur la sortie standard\n"

#: ../src/gui/gxmessage.c:1188
#, c-format
msgid  "  -center                Open the window in the center of the screen\n"
msgstr "  -center                Ouvrir la fenêtre au centre de l'écran\n"

#: ../src/gui/gxmessage.c:1189
#, c-format
msgid  "  -nearmouse             Open the window near the mouse pointer\n"
msgstr "  -nearmouse             Ouvrir la fenêtre près du pointeur de la souris\n"

#: ../src/gui/gxmessage.c:1190
#, c-format
msgid  "  -timeout SECONDS       Exit with code 0 after SECONDS seconds\n"
msgstr "  -timeout SECONDES      Sortie avec le code 0 après SECONDES secondes\n"

#: ../src/gui/gxmessage.c:1191
#, c-format
msgid  "  -display DISPLAY       X display to use\n"
msgstr "  -display AFFICHAGE     Affichage X à utiliser\n"

#: ../src/gui/gxmessage.c:1192
#, c-format
msgid  "  -fn FONT | -font FONT  Set message font (works with GTK font names)\n"
msgstr ""
"  -fn | -font POLICE     Définir la police des messages (fonctionne avec les noms de polices "
"GTK)\n"

#: ../src/gui/gxmessage.c:1193
#, c-format
msgid  "  -fg COLOR              Set message font color\n"
msgstr "  -fg COULEUR            Définir la couleur de la police du message\n"

#: ../src/gui/gxmessage.c:1194
#, c-format
msgid  "  -bg COLOR              Set message background color\n"
msgstr "  -bg COULEUR            Définir la couleur d'arrière-plan du message\n"

#: ../src/gui/gxmessage.c:1196
#, c-format
msgid  "  -geometry GEOMETRY     Set window size (position will be ignored)\n"
msgstr "  -geometry GÉOMÉTRIE    Définir la taille de la fenêtre (la position sera ignorée)\n"

#: ../src/gui/gxmessage.c:1198
#, c-format
msgid  "  -iconic                Start iconified\n"
msgstr "  -icône                 de démarrage iconique\n"

#: ../src/gui/gxmessage.c:1199
#, c-format
msgid  "  -name NAME             Program name as used by the window manager\n"
msgstr "  -name NOM              Nom du programme tel qu'utilisé par le gestionnaire de fenêtres\n"

#: ../src/gui/gxmessage.c:1200
#, c-format
msgid  "  -title TITLE           Set window title to TITLE\n"
msgstr "  -title TITRE           Définir le titre de la fenêtre sur TITRE\n"

#: ../src/gui/gxmessage.c:1202
#, c-format
msgid  "Additional %s options:\n"
msgstr "Options supplémentaires %s:\n"

#: ../src/gui/gxmessage.c:1203
#, c-format
msgid  "  -borderless            Open the window without border decoration\n"
msgstr "  -borderless            Ouvrir la fenêtre sans bordure décorative\n"

#: ../src/gui/gxmessage.c:1205
#, c-format
msgid  "  -sticky                Make the window stick to all desktops\n"
msgstr "  -sticky                Permet de fixer la fenêtre sur tous les bureaux\n"

#: ../src/gui/gxmessage.c:1206
#, c-format
msgid  "  -ontop                 Keep window on top\n"
msgstr "  -ontop                 Garder la fenêtre en haut\n"

#: ../src/gui/gxmessage.c:1208
#, c-format
msgid  "  -nofocus               Don't focus the window when it opens\n"
msgstr "  -nofocus               Ne pas donner le focus à la fenêtre lors de son ouverture\n"

#: ../src/gui/gxmessage.c:1209
#, c-format
msgid  "  -noescape              Don't allow pressing ESC to close the window\n"
msgstr "  -noescape              Empêcher la fermeture de la fenêtre avec la touche ÉCHAP\n"

#: ../src/gui/gxmessage.c:1210
#, c-format
msgid  "  -encoding CHARSET      Expect CHARSET as the message encoding\n"
msgstr "  -encoding CHARSET      L'encodage du message doit être de type CHARSET.\n"

#: ../src/gui/gxmessage.c:1211
#, c-format
msgid  "  -entry                 Prompt for text to be sent to stdout\n"
msgstr "  -entry                 Invite de saisie de texte à envoyer à la sortie standard\n"

#: ../src/gui/gxmessage.c:1212
#, c-format
msgid  "  -entrytext TEXT        Same as -entry, but with TEXT as default text\n"
msgstr "  -entrytext TEXT        Identique à -entry, mais avec TEXT comme texte par défaut\n"

#: ../src/gui/gxmessage.c:1213
#, c-format
msgid  "  -wrap                  Wrap lines of text to fit window width\n"
msgstr ""
"  -wrap                  Retour à la ligne automatique pour s'adapter à la largeur de la fenêtre\n"

#: ../src/gui/gxmessage.c:1214
#, c-format
msgid  "  -help | -?             Show this usage information\n"
msgstr "  -help | -?             Afficher ces informations d'utilisation\n"

#: ../src/gui/gxmessage.c:1215
#, c-format
msgid  "  -version               Show gxmessage version and Copyright details\n"
msgstr ""
"  -version               Afficher la version de gxmessage et les informations relatives aux "
"droits d'auteur\n"

#: ../src/gui/gxmessage.c:1217
#, c-format
msgid  "Please report bugs to %s.\n"
msgstr "Veuillez signaler les bogues à %s.\n"

#: ../src/gui/gxmessage.c:1269
#, c-format
msgid  ""
"Copyright (©) %s %s\n"
"This is free software. You may redistribute copies of it under\n"
"the terms of the GNU General Public License <https://www.gnu.org/licenses/gpl.html>.\n"
"There is NO WARRANTY, to the extent permitted by law.\n"
msgstr ""
"Copyright (©) %s %s\n"
"This is free software. You may redistribute copies of it under\n"
"the terms of the GNU General Public License <https://www.gnu.org/licenses/gpl.html>.\n"
"There is NO WARRANTY, to the extent permitted by law.\n"

#: ../src/gui/gxmessage.c:1278
#: ../src/gui/gxmessage.c:1322
#, c-format
msgid  "%s: can't have both -entry and -print\n"
msgstr "%s: impossible d'avoir à la fois -entry et -print\n"

#: ../src/gui/gxmessage.c:1307
#: ../src/gui/gxmessage.c:1402
#, c-format
msgid  "%s: can't get message from both -file and command line\n"
msgstr ""
"%s: impossible de récupérer le message à la fois depuis le fichier et la ligne de commande\n"

#: ../src/gui/gxmessage.c:1331
#, c-format
msgid  "%s: integer -timeout value expected\n"
msgstr "%s: valeur de délai d'attente entière attendue\n"

#: ../src/gui/gxmessage.c:1431
#, c-format
msgid  "%s: unable to read file\n"
msgstr "%s: impossible de lire le fichier\n"

#: ../src/gui/gxmessage.c:1444
#, c-format
msgid  "%s: message text is required\n"
msgstr "%s: le texte du message est obligatoire\n"

#: ../src/gui/gxmessage.c:1445
#, c-format
msgid  "Try `%s --help' for more information\n"
msgstr "Essayez `%s --help` pour plus d'informations\n"

